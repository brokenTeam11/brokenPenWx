/* pages/gradGoodGoods/grabing/index.wxss */

page {
  height: 100%;
  background-color: #fff;
}

.main {
  height: 100%;
  position: relative;
}

swiper-item {
  box-sizing: border-box;
  padding: 15rpx 25rpx 66rpx;
  position: relative;
}

.m-content {
  height: 100%;
  box-shadow: 0rpx 8rpx 19rpx 0px rgba(126, 142, 158, 0.28);
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  position: relative;
}

.s-tag {
  display: inline-block;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  position: absolute;
  margin-left: -16rpx;
  z-index: 1;
}

.s-tag .tag-img {
  width: 223rpx;
  height: 249rpx;
}

.s-tag .tag-text {
  position: absolute;
  display: flex;
  flex-direction: column;
  text-align: center;
}

.s-tag .text1 {
  width: 183rpx;
  top: 13rpx;
  left: 21rpx;
}

.s-tag .text2 {
  width: 183rpx;
  top: 128rpx;
  left: 21rpx;
}

.m-content .top {
  display: flex;
  flex-direction: column;
  color: rgba(46, 47, 77, 1);
  padding: 10rpx 0 48rpx 207rpx;
}

.m-content .top .text1 {
  font-size: 33rpx;
  font-weight: 600;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.m-content .top .text2 {
  font-size: 29rpx;
  margin-top: 22rpx;
  font-weight: 300;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.m-content .bottom {
  flex: 1;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  position: relative;
  text-align: center;
}

.m-container {
  position: absolute;
  top: 250rpx;
  left: 0;
  right: 0;
  height: 35%;
  overflow: hidden;
}

.m-container .bullet {
  padding: 0 17rpx 0 11rpx;
  display: flex;
  background-color: rgba(0, 0, 0, 0.4);
  position: absolute;
  border-radius: 28rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 56rpx;
  align-items: center;
  white-space: nowrap;
}

@keyframes first {
  0% {
    left: 100%;
  }

  25% {
    left: -100%;
  }

  100% {
    left: -100%;
  }
}

.m-container image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
}

.m-container text {
  margin-left: 10rpx;
}

.s-grapButton {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}

.s-grapButton .item {
  margin: 0 auto;
  margin-bottom: 14rpx;
  width: 166rpx;
  height: 166rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: rgba(46, 47, 77, 1);
  box-shadow: 0rpx 10rpx 21rpx 0rpx rgba(117, 122, 174, 0.52);
  border-radius: 50%;
}

.s-grapButton text {
  font-size: 35rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.s-grapButton image {
  width: 59rpx;
  height: 50rpx;
  margin-bottom: 10rpx;
}

.m-content .rule {
  position: absolute;
  bottom: 290rpx;
  right: 0;
  background: rgba(98, 98, 98, 0.61);
  border-radius: 12rpx 0 0 12rpx;
  font-size: 21rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  letter-spacing: 2rpx;
  line-height: 43rpx;
  width: 56rpx;
  height: 86rpx;
  text-align: center;
}

.m-content .bottom .count {
  padding: 0 29rpx;
  margin-bottom: 15rpx;
  display: inline-block;
  background: rgba(0, 0, 0, 0.75);
  border: 1rpx solid rgba(106, 110, 148, 1);
  border-radius: 28rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 56rpx;
}

.m-mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(31, 32, 52, 0.5);
  z-index: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}

@keyframes display {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

.m-mask .content {
  display: inline-flex;
  background: rgba(230, 11, 48, 1);
  border-radius: 30rpx;
  color: #fff;
  font-size: 400;
  padding: 72rpx 30rpx 106rpx;
  flex-direction: column;
  align-items: center;
  letter-spacing: 2rpx;
  text-align: center;
}

.m-mask .hd {
  display: none !important;
}

.m-mask .text1 {
  font-size: 46rpx;
  line-height: 74rpx;
}

.m-mask .text2 {
  margin-top: 25rpx;
  font-size: 133rpx;
  color: rgba(255, 199, 18, 1);
}

.m-mask .text2>text {
  font-size: 82rpx;
}

.m-mask .text3, .m-mask .text4 {
  font-size: 31rpx;
}

.m-mask .text3 {
  margin-top: 50rpx;
}

.m-mask .text4 {
  margin-top: 30rpx;
}

.m-mask .button {
  background: rgba(250, 164, 28, 1);
  box-shadow: 0rpx 8rpx 19rpx 0rpx rgba(127, 17, 17, 0.24);
  border-radius: 52rpx;
  font-size: 36rpx;
  font-weight: 500;
  line-height: 104rpx;
  padding: 0 93rpx;
  margin-top: 79rpx;
}

.m-share {
  position: absolute;
  width: 100%;
  background-color: #fff;
  font-weight: 400;
  color: rgba(46, 47, 77, 1);
  font-size: 28rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e5e5e5;
  animation: bottomTop .3s linear forwards;
}

@keyframes bottomTop {
  0% {
    bottom: -60%;
  }

  100% {
    bottom: 0%;
  }
}

.m-share .top {
  display: flex;
  padding: 0 24rpx 0 27rpx;
}

.m-share .item {
  padding: 54rpx 0 85rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.m-share .top text {
  margin-top: 22rpx;
  line-height: 1.5;
}

.m-share .top button {
  font-size: inherit;
  border: none;
  display: flex;
  flex-direction: column;
  margin: 0;
  padding: 0;
  width: 100%;
  align-items: center;
  background: transparent;
}

.m-share .top button::after {
  border: none;
}

.m-share image {
  width: 80rpx;
  height: 80rpx;
}

.m-share .info {
  margin: 0 24rpx 0 27rpx;
  padding: 0 18rpx 41rpx 20rpx;
  line-height: 42rpx;
  border-bottom: 2rpx solid #e5e5e5;
}

.m-share .cancel {
  text-align: center;
  font-size: 34rpx;
  line-height: 97rpx;
}

.hover {
  background-color: #e5e5e5 !important;
}
