/* @import"./../../wxParse/wxParse.wxss"; */

page {
  background-color: rgba(31, 32, 52, 0.1);
}

.clf35554 {
  color: #f35554;
}

.wrap-b {
  background: transparent;
  width: 100%;
  min-height: 100%;
}

.fixed-background {
  background-image: linear-gradient(-180deg, #7b6f86 0%, #4b4154 100%);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: -1;
}

.detail-content {
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
  margin-bottom: 100rpx;
  background-color: #f2f2f3;
  box-sizing: border-box;
}

.detail-head {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}

.detail-head-swiper {
  width: 100%;
  height: 593rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.detail-head-name {
  font-family: PingFang SC;
  color: #353535;
  font-size: 35rpx;
  letter-spacing: 0.625px;
  line-height: 52rpx;
  font-weight: bold;
  margin-top: 30rpx;
}

.detail-head-info {
  display: flex;
  flex-direction: row;
  font-family: PingFang SC;
  font-weight: lighter;
  font-size: 29rpx;
  letter-spacing: 1.25px;
  line-height: 52rpx;
}

.detail-head-info view {
  margin-right: 10rpx;
}

.detail-head-info text {
  margin-left: 10rpx;
}

.detail-head-price {
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: #6a6e94;
  margin-bottom: 50rpx;
  letter-spacing: 1px;
}

/* 配送说明开始 */

.send-desc {
  display: flex;
  flex-direction: column;
  letter-spacing: 1px;
  background-color: #fff;
  margin: 27rpx 0;
  padding: 0 14rpx;
}

.send-desc-list {
  display: flex;
  flex-direction: row;
  padding: 20rpx 0;
}

.send-line {
  height: 2rpx;
  background: rgba(238, 238, 238, 1);
}

.send-desc-title {
  display: flex;
  flex: 74;
  font-size: 30rpx;
  font-family: PingFang-SC;
  font-weight: 300;
  color: rgba(0, 0, 0, 1);
  align-items: flex-start;
}

.send-list-senddesc {
  line-height: 43rpx;
}

.send-list-senddesc, .send-list-severlist {
  display: flex;
  flex: 301;
  font-size: 24rpx;
  font-family: PingFang-SC;
  font-weight: 300;
  color: rgba(0, 0, 0, 1);
  margin-left: 22rpx;
}

.send-list-severlist {
  flex-direction: row;
}

.send-list-severitem {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-right: 13rpx;
}

.send-list-severimg {
  width: 40rpx;
  height: 39rpx;
}

.send-list-severdesc {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(124, 124, 124, 1);
  margin-left: 7rpx;
}

/* 配送说明结束 */

/* 顾客评价开始 */

.evaluation {
  margin: 0 0 27rpx;
  padding: 0 14rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
}

.evaluation-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 26rpx 0;
  letter-spacing: 1px;
}

.evaluation-title-left {
  font-size: 30rpx;
  font-weight: 300;
  font-family: PingFang-SC;
  color: rgba(0, 0, 0, 1);
}

.evaluation-title-right {
  font-size: 28rpx;
  font-family: PingFang-SC;
  color: rgba(239, 72, 56, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-right::after {
  content: '';
  display: block;
  width: 17rpx;
  height: 17rpx;
  border: 1px solid #979797;
  border-right: none;
  border-bottom: none;
  transform: rotate(135deg);
}

.evaluation-tip {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  font-size: 27rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  margin-bottom: 10rpx;
  color: rgba(80, 80, 80, 1);
}

.evaluation-tip-item {
  display: flex;
  align-items: center;
  justify-content: center;
  white-space: nowrap;
  height: 54rpx;
  background-color: rgba(239, 72, 56, 0.11);
  padding: 0 16rpx;
  border-radius: 27rpx;
  letter-spacing: 1px;
  margin-bottom: 16rpx;
}

.evaluation-list {
  display: block;
  margin-bottom: 37rpx;
  white-space: nowrap;
}

.evaluation-item {
  border: 1px solid rgba(49, 49, 49, 0.3);
  border-radius: 6rpx;
  width: 457rpx;
  height: 195rpx;
  display: inline-block;
  box-sizing: border-box;
  padding: 13rpx;
  padding-bottom: 21rpx;
  margin-right: 10rpx;
  vertical-align: top;
}

.evaluation-item-wp {
  display: flex;
  flex: 1;
  flex-direction: column;
}

.evaluator-wp {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.evaluator-avatar {
  width: 42rpx;
  height: 42rpx;
  border-radius: 50%;
  margin-right: 8rpx;
}

.evaluator-name {
  font-size: 22rpx;
  font-family: PingFang-SC;
  color: rgba(80, 80, 80, 1);
}

.evaluation-content {
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.evaluation-desc {
  width: 246rpx;
  font-size: 23rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(80, 80, 80, 1);
  line-height: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  overflow: hidden;
  white-space: normal;
}

.evaluation-desc.noImg {
  width: 100%;
}

.evaluation-img {
  width: 179rpx;
  height: 123rpx;
  object-fit: cover;
}

.more-comment {
  height: 195rpx;
  width: 195rpx;
  background-color: #e8e8e8;
  display: inline-block;
  box-sizing: border-box;
  font-family: PingFang-SC-Medium;
  font-size: 26rpx;
  text-align: center;
  color: #505050;
  vertical-align: top;
}

.more-content {
  display: flex;
  height: 195rpx;
  width: 195rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.comment-line {
  display: flex;
  margin: 10rpx 0;
  width: 160rpx;
  height: 1px;
  background-color: #505050;
}

/* 顾客评价结束 */

/* 商家介绍开始 */

.good-desc {
  background-color: #fff;
  display: flex;
  flex-direction: row;
  height: 165rpx;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 27rpx;
  padding: 0 26rpx;
  letter-spacing: 1px;
}

.good-left {
  display: flex;
  flex-direction: row;
}

.good-avatar {
  width: 118rpx;
  height: 108rpx;
  margin-right: 24rpx;
}

.good-avatar-img {
  width: 100%;
  height: 100%;
}

.good-info {
  display: flex;
  flex-direction: column;
  max-width: 437rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.good-name {
  display: flex;
  flex-direction: row;
}

.good-name-desc {
  font-size: 30rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
}

.good-kind {
  display: flex;
  justify-content: center;
  height: 34rpx;
  background: rgba(106, 110, 148, 1);
  border-radius: 17rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  padding: 0 8rpx;
  margin-left: 24rpx;
  color: rgba(255, 255, 255, 1);
}

.good-tip {
  font-size: 24.19rpx;
  margin-top: 20rpx;
  font-family: PingFang-SC-Light;
  font-weight: 300;
  color: rgba(80, 80, 80, 1);
}

.good-tip-desc {
  max-width: 437rpx;
  white-space: nowrap;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.good-tip-desc view:nth-child(n+2) {
  display: none;
}

.good-more {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
}

.good-more-img::after {
  content: '';
  display: block;
  width: 17rpx;
  height: 17rpx;
  border: 1px solid #979797;
  border-right: none;
  border-bottom: none;
  transform: rotate(135deg);
}

/* 商家介绍结束 */

/* 钻石参数介绍开始 */

.diamond-parameter {
  display: flex;
  flex-direction: column;
  padding: 0 20rpx;
  background-color: #fff;
  margin-bottom: 27rpx;
  letter-spacing: 1px;
}

.diamond-title {
  font-size: 30rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(31, 32, 52, 1);
  margin-top: 24rpx;
  margin-bottom: 48rpx;
}

.diamond-certificate {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.diamond-certificate-img {
  width: 254rpx;
  height: 77rpx;
}

.diamond-certificate-img image {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.diamond-certificate-info {
  display: flex;
  flex-direction: row;
  margin-top: 17rpx;
  margin-bottom: 40rpx;
}

.diamond-certificate-tip {
  font-size: 26rpx;
  font-family: PingFang-SC-Light;
  font-weight: lighter;
  color: rgba(31, 32, 52, 1);
}

.diamond-certificate-number {
  font-size: 27rpx;
  font-family: PingFang-SC;
  color: rgba(31, 32, 52, 1);
}

.diamond-parameter-list {
  display: flex;
  flex-direction: column;
}

.diamond-parameter-list-row {
  display: flex;
  flex-direction: row;
  border-top: 2.4rpx solid #eee;
  justify-content: space-between;
}

.diamond-parameter-list-item {
  flex: 1;
  height: 84rpx;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.diamond-parameter-list-item-name {
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  color: rgba(97, 97, 97, 1);
}

.diamond-parameter-list-item-value {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  color: rgba(0, 0, 0, 1);
}

/* 钻石参数介绍结束 */

/* 导航条开始 */

.detail-nav-wp.active {
  opacity: 1;
  visibility: visible;
  animation: top 1s cubic-bezier(0.23, 1, 0.320, 1);
}

@keyframes top {
  from {
    opacity: 0;
    transform: translateY(-30px);
  }

  to {
    opacity: 1;
    transform: translateY(0px);
  }
}

.detail-nav-wp {
  display: flex;
  opacity: 0;
  visibility: hidden;
  position: fixed;
  left: 0;
  top: 0;
  height: 112rpx;
  width: 100%;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);
}

.detail-nav {
  display: flex;
  width: 652rpx;
  height: 69rpx;
  margin: 0 auto;
}

.detal-nav-item {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  font-size: 27rpx;
  font-family: PingFang-SC-Regular;
  color: #000;
  letter-spacing: 1px;
  transition: background-color 0.3s cubic-bezier(0.25, 0.1, 0.3, 1.5);
  border: 1px solid rgba(125, 125, 125, 0.5);
  border-right: none;
}

.detal-nav-item:last-child {
  border-right: 1px solid rgba(125, 125, 125, 0.5);
}

.detal-nav-item.active {
  color: #fff;
  background-color: #1f2034;
  border: none;
}

/* 导航条结束 */

/* 底部开始 */

.detail-footer {
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 109rpx;
  box-shadow: 0 1px 10px rgba(0, 0, 0, 0.1);
  letter-spacing: 1.25px;
  overflow: visible;
}

.detail-footer-left {
  display: flex;
  flex: 1;
  height: 100%;
  align-items: center;
  flex-direction: row;
  justify-content: space-around;
  background-color: #fff;
}

.detail-footer-left-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.detail-footer-image-wp {
  width: 43rpx;
  height: 40rpx;
  display: block;
  position: relative;
}

.detail-footer-image-home {
  width: 40rpx;
  height: 37rpx;
  background: url(https://kpzuan.com/api/image/btn_my@3x.png) no-repeat center;
  background-size: 40rpx 37rpx;
  position: absolute;
  bottom: 0;
}

.detail-footer-image-home.current {
  background: url(https://kpzuan.com/api/image/btn_gmy@3x.png) no-repeat center;
  background-size: 40rpx 37rpx;
}

.detail-footer-image-collection {
  width: 39rpx;
  height: 35rpx;
}

.detail-footer-image-car {
  width: 43rpx;
  height: 40rpx;
  background: url(https://kpzuan.com/api/image/btn_shopping@3x.png) no-repeat center;
  background-size: 43rpx 40rpx;
  position: absolute;
  left: -5rpx;
}

.detail-footer-image-car.current {
  background: url(https://kpzuan.com/api/image/btn_gshopping@3x.png) no-repeat center;
  background-size: 43rpx 40rpx;
}

.detail-footer-left-item {
  flex: 1;
  padding-top: 5rpx;
}

.detail-footer-left-item text {
  font-size: 21.74rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(97, 97, 97, 1);
  margin-top: 6rpx;
}

.detail-footer-right {
  width: 432rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #fff;
}

.detail-footer-car, .detail-footer-buy {
  width: 212rpx;
  height: 71rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-family: PingFang-SC;
  color: rgba(255, 255, 255, 1);
  background-color: #6a6e94;
}

.detail-footer-car {
  background-color: #1f2034;
  border-radius: 41rpx 0 0 41rpx;
}

.detail-footer-buy {
  background-color: #6a6e94;
  border-radius: 0 41rpx 41rpx 0;
}

.detail-footer-active {
  width: 424rpx;
  height: 71rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  font-family: PingFang-SC;
  color: rgba(255, 255, 255, 1);
  background-color: #6a6e94;
  border-radius: 41rpx;
}
.detail-footer-active.bg-bf{
  background-color: #bfbfbf;
}

/* 底部结束 */

/* 钻戒参数开始 */

.diamond-ring-parameter {
  background-color: #fff;
  display: flex;
  flex-direction: column;
  padding: 32rpx 26rpx 20rpx;
  letter-spacing: 1px;
}

.diamond-ring-parameter-item {
  display: flex;
  flex-direction: column;
}

.diamond-ring-parameter-item-title {
  font-family: PingFang SC;
  font-size: 27rpx;
  height: 62rpx;
  display: flex;
  align-items: center;
}

.diamond-ring-parameter-item-desc {
  font-family: PingFang SC;
  font-size: 26rpx;
  min-height: 62rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-bottom: 2rpx solid #eee;
}

.diamond-ring-parameter-item-desc-title {
  width: 180rpx;
  font-weight: lighter;
}

.diamond-ring-parameter-item-title.underline {
  border-bottom: 2rpx solid #eee;
}

.diamond-ring-parameter-item-desc.none {
  border: none;
}

/* 图文详情 */

.detial-img {
  display: flex;
  flex-direction: column;
  background-color: #fff;
  font-size: 26rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  line-height: 43rpx;
  word-break: break-all;
  overflow: hidden;
}

.detial-img view {
  word-break: break-all;
  overflow: hidden;
}

.detial-img image {
  width: 100%;
  margin-top: -10rpx;
  padding: 0;
}

/* 钻戒参数结束 */

/* 在线客服开始 */

.m-customerservice {
  width: 63rpx;
  position: fixed;
  top: 712rpx;
  right: 0;
  background: rgba(106, 110, 148, 1);
  opacity: 0.87;
  z-index: 2;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 25rpx 0 21rpx 0;
  border-radius: 8rpx 0 0 8rpx;
}

.m-customerservice button {
  margin: 25rpx 0 21rpx 0;
  background-color: transparent;
  padding: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.m-customerservice button::after {
  border: 0px;
}

.m-customerservice image {
  width: 39rpx;
  height: 36rpx;
}

.m-customerservice text {
  margin-top: 17rpx;
  font-size: 25.2rpx;
  font-weight: 400;
  letter-spacing: 8rpx;
  color: rgba(255, 255, 255, 1);
  writing-mode: vertical-lr;
}

/* 在线客服结束 */

/* 购物车动画 */

.car-animation {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  animation: goods 1.5s cubic-bezier(0.25, 0.1, 0.3, 1.5);
  animation-fill-mode: forwards;
  position: absolute;
  left: -50rpx;
  top: -700rpx;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.1);
  overflow: hidden;
  letter-spacing: 0;
}

.good-image {
  width: 100%;
}

.detail-footer-left-item .carcount {
  display: block;
  min-width: 34rpx;
  height: 34rpx;
  background: rgba(237, 118, 108, 1);
  border-radius: 17rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Bold;
  font-weight: 300;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 34rpx;
  position: absolute;
  letter-spacing: 0;
  right: 15rpx;
  top: -15rpx;
}

.add-car-hover {
  color: #6a6e94 !important;
}

.buy-hover {
  color: #2e2f4d !important;
}

@keyframes goods {
  form {
    opacity: 1;
  }

  to {
    width: 0;
    height: 0;
    left: 35rpx;
    top: 18rpx;
    opacity: 0;
  }
}

/* 购物车动画 */

.m-active {
  box-sizing: border-box;
  width: 100%;
  position: relative;
  background-color: rgba(106, 110, 148, 1);
  display: flex;
  color: #fff;
  padding: 20rpx 28rpx 20rpx 37rpx;
  align-items: center;
  z-index: 1;
}

.m-active .left-text1 {
  font-size: 34rpx;
}

.m-active .left-text2 {
  font-size: 51rpx;
}

.m-active .center {
  flex: 1;
  width: 0;
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  margin-left: 34rpx;
  word-break: break-all;
}

.m-active .right {
  font-size: 26rpx;
  color: rgba(46, 47, 77, 1);
  background-color: #fff;
  height: 55rpx;
  line-height: 55rpx;
  padding: 0 31rpx;
  border-radius: 31rpx;
}

.m-active .background {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(230, 11, 48, 1);
  z-index: -1;
}

.m-active .org_box {
  position: absolute;
  line-height: 62rpx;
  font-size: 26rpx;
  background-color: rgba(101, 101, 101, 1);
  left: 20rpx;
  top: -80rpx;
  padding: 0 19rpx;
  border-radius: 29rpx;
  white-space: nowrap;
}

.m-active .org_bot_cor {
  width: 0;
  height: 0;
  font-size: 0;
  border-width: 16rpx 10rpx;
  border-style: solid;
  border-color: rgba(101, 101, 101, 1) transparent transparent;
  _border-color: rgba(101, 101, 101, 1) white white;
  overflow: hidden;
  position: absolute;
  bottom: -28rpx;
  left: 60rpx;
}

.m-mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(31, 32, 52, 0.5);
  z-index: 2;
  animation: display 1s linear forwards;
  display: flex;
  justify-content: center;
  align-items: center;
}

@keyframes display {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

.m-mask .model {
  display: inline-flex;
  background: #fff;
  border-radius: 18rpx;
  color: #2e2f4d;
  font-size: 400;
  flex-direction: column;
  letter-spacing: 2rpx;
  text-align: center;
  width: 553rpx;
  overflow: hidden;
}

.m-mask .title {
  font-size: 30rpx;
  font-weight: 500;
  padding: 24rpx 0;
  border-bottom: 2rpx solid #eee;
}

.m-mask .content {
  padding: 43rpx 30rpx 52rpx;
  font-size: 25rpx;
  text-align: left;
  word-break: break-all;
  border-bottom: 2rpx solid #eee;
}

.m-mask .bottom {
  display: flex;
  align-items: center;
}

.m-mask .bottom>view:not(.border) {
  padding: 23rpx 0 30rpx;
  flex: 1;
  width: 0;
  font-size: 29rpx;
}

.m-mask .border {
  width: 2rpx;
  height: 52rpx;
  background-color: #eee;
}

.m-mask .hover {
  background-color: #eee;
}
