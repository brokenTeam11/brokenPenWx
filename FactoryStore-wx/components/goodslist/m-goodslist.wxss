.m-goodlist .topimg {
  text-align: center;
}

.m-goodlist .topimg image {
  width: 197rpx;
  height: 92rpx;
}

.m-goodlist .content {
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
}

.m-goodlist .divder {
  margin: 0 auto;
  margin-top: 10rpx;
  width: calc(658 / 750 * 100%);
  height: 5rpx;
  background-color: #eee;
}

.double .item {
  width: 50%;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.double .item-slc {
  background-color: #eee;
}

.double .warp {
  width: 100%;
}

.double .img-box {
  width: 100%;
  height: 0px;
  padding-bottom: 100%;
  position: relative;
}

.double .img-box image {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: transparent;
}

.double .goodsname {
  margin-top: 25rpx;
  font-size: 27.15rpx;
  font-weight: 400;
  line-height: 47rpx;
  color: rgba(97, 97, 97, 1);
  text-align: center;
}

.double .goodsprice {
  font-size: 27.15rpx;
  font-weight: 400;
  line-height: 58rpx;
  color: #6a6e94;
  text-align: center;
}

.single .item {
  padding: 30rpx 62rpx;
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: row;
}

.single .item image {
  width: 112rpx;
  height: 112rpx;
}

.single .goodstitle {
  display: flex;
  flex-direction: column;
  margin-left: 69rpx;
  flex: 1;
  width: 0;
}

.single .goodsname {
  font-size: 25.94rpx;
  font-weight: 300;
  line-height: 51.89rpx;
  color: #616161;
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.single .goodsprice {
  font-size: 36.2rpx;
  font-weight: 700;
  line-height: 51.89rpx;
  color: rgba(46, 47, 77, 1);
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.m-noMore {
  display: flex;
  align-items: center;
  flex-direction: column;
}

.m-noMore image {
  margin-top: 182rpx;
  width: 314rpx;
  height: 250rpx;
}

.m-noMore .text1 {
  margin-top: 81.06rpx;
  font-size: 30.17rpx;
  font-weight: 300;
  color: rgba(46, 47, 77, 1);
  line-height: 82.05rpx;
}
